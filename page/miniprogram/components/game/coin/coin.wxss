.guize{
  width: 100%;
  height: 100vh;
  position: absolute;
  z-index: 50;
  top: 0;
  left: 0;
  background: #000;
}

.guize{
  text-align: center;
  font-size: 40rpx;
  font-weight: bold;
  padding: 50rpx;
  padding-top: 160rpx;
}
.guize .title{
  font-size: 60rpx;
  color: #0f6cb4;
}
.guize .wu{
  font-size: 60rpx;
  color: #ddd;
  margin-top: 80rpx;
}
.guize view{
  color: #fff;
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 50rpx;
}
.guize .lie{
  color: rgb(240, 52, 52);
  font-size: 30rpx;
}
@keyframes guizeAs{
  0%{
    option: 1;
    transform: scale(1) rotate(0);
  }
  50%{
    option: 1;
  }
  100%{
    opacity: 0;
    transform: scale(0) rotate(360deg);
  }
}
.guizeAs{
  animation: guizeAs 2s linear 5s;
  animation-fill-mode: forwards;
}
