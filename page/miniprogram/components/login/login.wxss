.loginBox{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100vh;
  background: rgba(1,1,1,0.6);
  z-index: 999;
  padding-top:250rpx;
}
.boxes{
  width: 550rpx;
  height: 650rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 20rpx;
  padding: 30rpx;
}
.boxesBtn button{
  margin: 0;
  padding: 0;
  width: 100%;
  line-height: 78rpx;
  color: #fff;
  background: #0f6cb4;
}
.boxesBtn{
  line-height: 60rpx;
  text-align: center;
  border-radius: 10rpx;
  background: #ddd;
  margin-top: 10rpx;
  color: #fff;
}
.boxesImg{
  width: 100%;
  height: 420rpx;
}