.coinBg{
  width: 100%;
  height: 100vh;
  background-color: rgba(1,1,1,0.5)
}
.rotate-wrap {
  width: 100px;
  height: 100px;
  transform-style: preserve-3d;
  position: absolute;
  left: 50%;
  margin-left: -100rpx;
  bottom: 0;
  transform: rotateX(-120deg);
}
.rotate-wrap .front, .rotate-wrap .reverse{
  width: 100px;
  height: 100px;
  background-size: cover;
  
}
.rotate-wrap .circle{
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.circle{
  border-radius: 50%;
  overflow: hidden;
}
.line{
  height: 200px;
  width: 1px;
  position:absolute;
  top: 0;
  left: 50%;
  background: red;
  transform: translateZ(0px);
}

@keyframes pao0 {
  0%{
  transform: rotateX(0deg) rotateY(180deg);
  bottom: 0;
  }
  10%{
  transform: rotateY(360deg) ;
  }
  20%{
  transform: rotateY(0deg) ;
  }
  30%{
  transform: rotateY(360deg) ;
  }
  40%{
  transform: rotateY(0deg);
  }
  50%{
  transform: rotateY(360deg) rotateX(360deg);
  bottom: 800rpx;
  }
  52%{
  transform: rotateX(-80deg) ;
  bottom: 800rpx;
  }
  55%{
  transform: rotateX(180deg);
  bottom: 800rpx;
  }
  60%{
  transform: rotateX(-180deg);
  bottom: 750rpx;
  }
  80%{
  transform: rotateX(-360deg) ;
  }
  90%{
  transform: rotateX(60deg) ;
  bottom: 0rpx;
  }
  100% {
  transform: rotateX(60deg);
  }
}
/* 用户赢 */
.pao0{
  animation: pao0 3s linear;
  animation-fill-mode: forwards;
}

