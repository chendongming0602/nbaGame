.picBox{
  width: 100%;
  height: 100vh;
  position: relative;
}
.picBg{
  width: 100%;
  height: 100%;
  position: absolute;
}
.pic{
  width: 100px;
  height: 100px;
  position: absolute;
  border-radius: 50%;
  z-index: 66;
  border: 2px solid #333;
}
.can{
  position: absolute;
  width: 100%;
  height: 100vh;
  top: -1000px;
}
.baocun{
  position: absolute;
  z-index: 66;
  top: 200rpx;
  padding: 50rpx;
  background: #f0f;
}
.btn{
  position: absolute;
  bottom: 80rpx;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 50rpx;
}
.btn view{
  line-height: 70rpx;
  background: rgb(21, 130, 150);
  color: #fff;
  width: 40%;
  text-align: center;
  font-size: 30rpx;
  border-radius: 10rpx;
}
.input{
  height: 68rpx;
  text-align: center;
  border: 2px solid #666;
  margin: 0 15rpx;
  font-size: 35rpx;
  font-weight: bold;
  border-radius: 5rpx;
  /* color:  */
}
.btn .btn1{
  background: linear-gradient(180deg,rgb(17, 117, 134) 0%,rgb(137, 217, 231) 100%);
}
.count{
  position: absolute;
  width: 100%;
  z-index: 6;
  font-size: 60px;
  text-align: center;
  font-weight: bold;
  /* top: 450rpx; */
  color: #2390e4;
}
.topScrll{
  position: absolute;
  top: 50rpx;
  left: 0;
  width: 100%;
  text-align: center;
  color: #666;
  z-index: 6;
}
@keyframes topScrllA{
  0%{
    left: 80%;
  }
  100%{
    left: -80%;
  }
}
.topScrllA{
  animation: topScrllA 5s linear infinite;
}